package alphonsoTree;

public class Node {
	
	Node left;
	Node right;
	char ch;
	int val;
	
	Node(){
		ch = 'G';
	}
	Node(char ch){
		this.ch = ch;
	}
	
	Node(int val){
		this();
		this.val  = val;
	}
	@Override
	public String toString() {
		return "Node [ch=" + this.ch + ", val=" + val + "]";
	}

	
	

}
